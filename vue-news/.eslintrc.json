{
	"env": {
		"es6": true
	},
	"extends": [ "plugin:vue/recommended" ],
	"rules": {
		"semi": [ "error", "always" ],
		"quotes": [ "error", "single" ],
		"no-unused-vars": "error",
		"no-console": "off",
		"object-curly-newline": [
			"error",
			{
				"ObjectExpression": "always",
				"ObjectPattern": {
					"multiline": true
				},
				"ImportDeclaration": "never",
				"ExportDeclaration": {
					"multiline": true,
					"minProperties": 3
				}
			}
		]
	},
	"parserOptions": {
		"parser": "babel-eslint",
		"sourceType": "module"
	}
}
